{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": "error",
        "indent": ["error", 4],
        "quotes": [1, "backtick", { "avoidEscape": true }],
        "linebreak-style": 0
    }
}
